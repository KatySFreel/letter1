<mjml>
   <mj-head>
		<mj-font name="Montserrat" 
      href="https://fonts.googleapis.com/css?family=Montserrat:regular,500,600,700" />
		<mj-title>Yorso</mj-title>
		<mj-preview>Yorso</mj-preview>
		<mj-style>
         *{
            font-family: 'Montserrat', 'Arial', serif !important;
         }
			h1 {
            font-family: 'Montserrat', 'Arial', serif !important;
				margin: 0px !important;
            line-height: 40px !important;
            color: #363636 !important;
            font-size: 32px !important;
            font-weight: 600 !important;
            text-align: center !important;
            letter-spacing: -1px !important;
			}
			.wrap {
				max-width: 352px !important
			}
         span{
            font-family: 'Montserrat', 'Arial', serif !important;
            text-decoration: underline !important;
            font-size: 32px !important;
            margin: 0px !important;
            line-height: 40px !important;
         }
         a { 
            font-family: 'Montserrat', 'Arial', serif !important;
            text-decoration: none;
            color: #232323 !important;
            font-size: 14px !important;
            line-height: 20px !important;
            transform: all .2s ease !important;
         } 
         .contact td {
            padding: padding: 0px !important;
         } 
         .footer {
            max-width: 538px !important;
            padding-left: 64px !important;
            padding-right: 64px !important;
         } 
         @media(max-width: 572px){
            .button table {
               width: 80% !important;
            }
            .button p {
               width: 75% !important;
            }
            .button td {
               width: 80% !important;
            }
            .offer td {
               padding: 26px 15px 24px !important;
            }
            .size {
               width: 80% !important;
            }
            h1 {
               font-size: 25px !important;
            }
            h1 span {
               font-size: 25px !important;
            }
            .wrap__title div {
               font-size: 18px !important;
            }
            .follow-wrap {
               width: 80% !important;
            }
         } 
         @media(max-width: 450px){
            .offer td {
               padding: 19px 10px 15px !important;
            }
            h1 {
               font-size: 25px !important;
            }
            h1 span {
               font-size: 25px !important;
            }
            .wrap {
               width: 90% !important;
            }
            .wrap__title div {
               font-size: 16px !important;
               padding: 5px !important;
            }
            .wrap td {
               padding: 5px !important;
            }
            .wrap th {
               font-size: 16px !important;
               padding: 10px !important;
            }
            .wrap th span {
               font-size: 15px !important;
               padding: 0px !important;
            }
            .wrap td {
               font-size: 16px !important;
               padding: 10px !important;
            }
            .help .help-bl  {
               padding: 0px 30px 30px 30px !important;
            }
            .help .help-color {
               padding: 30px 30px 0px !important;
            }
         }  
         @media(max-width: 380px){
            .wrap td {
               font-size: 12px !important;
               padding: 9px !important;
            }
            .wrap th {
               font-size: 12px !important;
               padding: 10px !important;
            }
            .wrap th span {
               font-size: 12px !important;
            }
            .wrap td {
               font-size: 12px !important;
               padding: 9px !important;
            }
            .button p {
               width: 85% !important;
               font-size: 12px !important;
               padding: 10px !important ;
            }
            .button table {
               width: 80% !important;
               padding-bottom: 15px !important;
            }
            .help .help-bl div{
               font-size: 14px !important
            }
            .help .help-color div{
               font-size: 14px !important
            }
            .offer__text div {
               font-size: 14px !important
            }
            .contact__wrap td {
               padding: 13px 0px 5px !important;
            }
            .contact__text {
               font-size: 12px !important;
            }
            .footer {
               padding-left: 15px !important;
               padding-right: 15px !important;
            } 
            .footer td {
               padding: 10px !important;
            }
         }  
         @media(max-width: 320px){
            h1 {
               font-size: 18px !important;
               line-height: 25px !important;
            }
            h1 span {
               font-size: 18px !important;
               line-height: 25px !important;
            }
            .wrap__title div {
               font-size: 14px !important;
               padding: 10px !important;
            }
            .offer td {
               padding: 15px 10px 6px !important;
            }
            .offer__text td {
               font-size: 14px !important;
               padding-bottom: 0px !important;
            }            
            .wrap td {
               font-size: 12px !important;
               padding: 5px !important;
            }
            .wrap th {
               font-size: 12px !important;
               padding: 5px !important;
            }
            .download__title div {
               font-size: 12px !important;
            }
            .download-button td{
               padding: 5px 0px 0px 0px !important;
            }
            .follow div {
               font-size: 12px !important;
            }
            .link {
               font-size: 12px !important;
            }
            .footer {
               padding-left: 5px !important;
               padding-right: 5px !important;
            } 
            .footer__title div {
               font-size: 12px !important;
            }
            .footer__title div a {
               font-size: 12px !important;
            }
         }   
		</mj-style>
   </mj-head>
   <mj-body width="664px" background-color="#F5F5F5">
      <mj-section background-color="#F5F5F5" padding="0px">
         <mj-column background-color="#F5F5F5">
         <!--noindex-->   
            <mj-image href="https://yorso.com/ru" target="_blank" width="112px" height="24px" align="center" padding="32px 0px" src="https://i.ibb.co/vXjRjNn/Logo.png" alt="Yorso"></mj-image>
         <!--/noindex-->
         </mj-column>
      </mj-section>
      <mj-section background-color="#F5F5F5" padding="0 32px">
         <mj-column css-class="offer" padding="64px 72px 24px" background-color="#ffffff">
            <mj-text css-class="offer__text" font-family="Montserrat" padding-bottom="24px" color="#363636" font-size="16px" align="center" line-height="24px" letter-spacing="0.1px">
            Dear Dima,</mj-text>
            <mj-text font-family="Montserrat" padding="0px 0px 24px 0px">
            <h1 font-family="Montserrat">
               <span>Artem</span> from <span>GARCIMAR</span> sent a request to access prices.
            </h1>
            </mj-text>
            <mj-text css-class="offer__text" font-family="Montserrat" color="#363636" font-size="16px" align="center" line-height="24px" letter-spacing="0.1px">
               Access is requested to the product:
            </mj-text>
         </mj-column>
      </mj-section>
      <mj-wrapper background-color="#F5F5F5" padding="0px 32px">
         <mj-section  background-color="#fff" padding-bottom="48px">
            <mj-column css-class="wrap" background-color="#FFF7EF" border-radius="12px">
               <mj-text css-class="wrap__title" font-family="Montserrat" align="center" padding="30px" color="#232323" font-size="20px" font-weight="600" align="center" line-height="28px">
                  Sea urchin caviar, chilled                    
               </mj-text>
               <mj-image css-class="size" padding="0px 30px" height="148px" border-radius="12px" align="center" src="https://i.ibb.co/6txFck6/Image.webp" alt="yorso"></mj-image>
               <mj-table padding="32px 45px" align="center">
                  <tr style="font-family: Montserrat;">
                     <th css-class="table" style="font-weight: 600;font-size: 18px;line-height: 24px; text-align:left;padding: 0 21px 8px 0;">$300/kg</th>                     
                     <th style="font-size: 14px; font-weight: 500; line-height: 20px; text-decoration-line: none; color: #736E6E;text-align: left">1 000 - 5 000 kg</th>
                  </tr>
                  <tr>
                     <td css-class="table" style="font-weight: 600;font-size: 18px;line-height: 24px; text-align:left;padding: 0 21px 8px 0;">$250/kg</td>
                     <td style="font-size: 14px; font-weight: 500; line-height: 20px; text-decoration-line: none; color: #736E6E;text-align: left">5 000 - 10 000 kg</td>
                  </tr>
                  <tr css-class="table" style="font-family: Montserrat;">
                     <th style="font-weight: 600;font-size: 18px;line-height: 24px; text-align:left;padding: 0 21px 0px 0;">$200/kg</th>                     
                     <th style="font-size: 14px; font-weight: 500; line-height: 20px; text-decoration-line: none; color: #736E6E; text-align: left">10 000 - 500 000 kg</th>
                  </tr>
               </mj-table>
               <mj-button css-class="button" width="292px" padding="0px 0px 30px 0px" font-family="Montserrat" background-color="#FF7800"  align="center" text-transform="uppercase" font-weight="600" border-radius="8px" height="56px">
                  Grant permission
               </mj-button>
            </mj-column>
         </mj-section>
      </mj-wrapper>
      <mj-section background-color="#F5F5F5" padding="0 32px">
         <mj-column css-class="help" border-top="1px #ECEAEA solid" background-color="#ffffff"  border-bottom="1px #ECEAEA solid" background-color="#ffffff">
            <mj-text css-class="help-color" font-family="Montserrat" padding="48px 72px 0px" color="#D66704" font-size="16px" align="center" line-height="28px" letter-spacing="0.1px">
            Something went wrong?</mj-text>
            <mj-text css-class="help-bl" font-family="Montserrat" padding="0px 72px 56px" color="#363636" font-size="16px" align="center" line-height="28px" letter-spacing="0.1px">
            Feel free to contact us via any links below.</mj-text>
         </mj-column>
      </mj-section>
